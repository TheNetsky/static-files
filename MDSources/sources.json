  "sources": {
    "toonily.com": {
      "id": "toonily.com",
      "domain": "https://toonily.com",
      "adapter": "madara",
      "enabled": true,
      "override": {
        "request": {
          "usePostId": true,
          "directoryPath": "webtoon",
          "directoryListingPath": "webtoons"
        },
        "selector": {
          "searchDirectoryListing": "$('div.page-item-detail.manga')",
          "mangaPageAltTitles": "$('h5:contains(Alt Name(s))').parent().next()"
        }
      }
    },
    "manga18sx.com": {
      "id": "manga18fx.com",
      "domain": "https://manga18fx.com",
      "adapter": "madara",
      "alias": [
        "https://manga18sx.com",
        "https://manga18fx.com"
      ],
      "enabled": true,
      "override": {
        "request": {
          "usePostId": false,
          "searchPath": "search?q=",
          "directoryListingPath": "manga-genre/manhwa",
          "directoryListingParam": "?orderby=latest"
        },
        "selector": {
          "searchDirectoryListing": "$('div.listupd > div.page-item')",
          "chapterListItem": "$('li.a-h')",
          "directoryListingItem": "$('div.listupd > div.page-item')"
        }
      }
    },
